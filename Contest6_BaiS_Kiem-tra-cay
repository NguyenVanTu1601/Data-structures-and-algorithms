#include<iostream>
#include<bits/stdc++.h>
using namespace std;
int a[1005][1005];
int check[1005];
int n;
void DFS(int u){
	check[u] = 0;
	for(int v = 1; v <= n; v++){
		if(check[v] == 1 && a[u][v] == 1) DFS(v);
	}
}

main(){
	int t;
	cin >> t;
	
	while(t--){
		cin >> n;
		for(int i = 1; i <= n; i++){
			check[i] = 1;
			for(int j = 1; j <= n; j++){
				a[i][j] = 0;
			}
		}
		int x, y;
		for(int i = 1; i <= n - 1; i++){
			cin >> x >> y;
			a[x][y] = 1;
			a[y][x] = 1;
		}
		int dem = 0;
		for(int i = 1; i <= n; i++){
			if(check[i] == 1){
				dem++;
				DFS(i);
			}
		}
		if(dem == 1) cout << "YES" << endl;
		else cout << "NO" << endl;
	}
    return 0;
}
