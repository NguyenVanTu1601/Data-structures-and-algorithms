#include<iostream>
#include<bits/stdc++.h>
using namespace std;
void Cong(int a, string s1, string s2){
	while(s1.length() != s2.length())
    {
    	if(s1.length() > s2.length()) s2 = '0' + s2; 
    	else s1 = '0' + s1;	
	}
	
	
	int temp = 0;
	int i = s1.length() - 1;
	string sum = "";
	while(i >= 0)
	{
		if(i > 0)
		{
			int s = (s1[i] - '0' ) + (s2[i] - '0') + temp;
			if(s >= a)
			{ 
			    char x = s % a + '0';
				sum = sum + x;
				temp = 1;
				i--;
			}
			
			else 
			{
				char x = s + '0';
			    sum = sum + x;
			    temp = 0;
			    i--;
			}
		}
		if(i == 0)
		{
			int s = (s1[i] - '0' ) + (s2[i] - '0') + temp;
			if(s >= a)
			{
				int m = s % a;
				int n = s / a;
				char x = (m + '0');
				char y = n + '0';
				sum = sum + x;
				sum = sum + y;
				break;
			}
			else
			{
			    char x = s + '0';
			    sum = sum + x;
		        break;
		    }
		}
	}
	//cout << s1 << endl << s2<< endl; 
	for(int i = sum.length() - 1; i >= 0; i--)
	{
		cout << sum[i];
	}
	
	cout << endl;
}
main(){
	int a;
	string x;
	string A;
	string B;
	cin >> a;
	cin >> A >> B;
	Cong(a,A,B);
    return 0;
}
