#include<iostream>
#include<bits/stdc++.h>
using namespace std;
int kiemtra(string s){
	stack <char> q;
	for(int i = 0; i < s.length(); i++){
		if((s[i] == '(' ) || (s[i] == '[') || s[i] == '{') q.push(s[i]);
		else {
			if(!q.empty()){
				if(s[i] == ')'){
					if(q.top() != '(') return 0;
			    }
			    if(s[i] == ']'){
			    	if(q.top() != '[') return 0;
				}
				if(s[i] == '}'){
			    	if(q.top() != '{') return 0;
				}
				q.pop(); // sau khi kiem tra neu k retrun 0 thi no dung -> xoa no di
			}
			else return 0; // TH ]{())
		}
    }
    
    return q.empty() == 1;
}

main(){
	string s;
	int t;
	cin >> t;
	string a;
	getline(cin,a); // xoa bo dem
	while(t--){
		getline(cin,s);
		if(kiemtra(s) == 1) cout <<"YES" << endl;
		else cout << "NO" << endl;
	}
    return 0;
}
