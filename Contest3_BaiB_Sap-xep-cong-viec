#include<iostream>
#include<bits/stdc++.h>
using namespace std;
struct congViec{
	int start;
	int finish;
};
bool cmp(congViec a, congViec b){
	return a.finish < b.finish;
}

main(){
	int t;
	cin >> t;
	int n;
	while(t--){
		cin >> n;
		congViec a[n + 1];
		for(int i = 0; i < n; i++){
			cin >> a[i].start >> a[i].finish;
		}
		sort(a,a+n,cmp);
//		for(int i = 0; i < n; i++){
//			cout << a[i].start << " " << a[i].finish << endl;
//		}
		int dem = 1;
		int i = 0;
		for(int j = 1; j < n ; j++){
			if(a[i].finish <= a[j].start) {
				dem++;
				i = j;
				//cout << a[i].start << " " << a[i].finish << endl;
			}
			
		}
		cout << dem << endl;
	}
    return 0;
}
