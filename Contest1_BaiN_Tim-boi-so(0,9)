#include<iostream>
#include<bits/stdc++.h>
using namespace std;
bool check = false;
void Sinh(int n, int a[20]){
	int i = n;
	while(i > 0 && a[i] == 9) {
		a[i] = 0;
		i--;
	}
	if(i <= 0) check = true;
	else{
		a[i] = 9;
	}
}
void in(int n, int a[]){
	for(int i = 1; i <= n; i++){
		cout << a[i] << " ";
	}
	cout << endl;
}

long long Sum(int n,int a[]){
	long long sum = 0;
	for(int i = 1; i <= n; i++){
		sum = sum * 10 + a[i];
	}
	return sum;
}
main(){
	int t;
	cin >> t;
	int x;
	while(t--){
		cin >> x;
		//if(9 % x == 0) cout << "9" << endl;
		//else{
			for(int n = 1; n <= 20; n++){
			    int a[20];
			    for(int i = 1 ; i < n; i++){
					a[i] = 0;
				}
				a[n] = 9;
					 check = false;
					int kt = 1;
					while(check == false){
						if(Sum(n,a) % x == 0){
							cout << Sum(n,a) << endl;
							kt = 0;
							break;
						}
						Sinh(n,a);
					}
					if(kt == 0) break;
				}
		    //}
		}
		

    return 0;
}
